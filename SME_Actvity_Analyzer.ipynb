        "# MANAGEMENT TOOL FINDER #\n",
        "\n",
        "from google.colab import drive\n",
        "import os\n",
        "import pdfplumber\n",
        "import re\n",
        "import nltk\n",
        "from nltk.tokenize import sent_tokenize\n",
        "from nltk.sentiment.vader import SentimentIntensityAnalyzer\n",
        "from collections import Counter\n",
        "\n",
        "# Mount Google Drive\n",
        "drive.mount('/content/drive')\n",
        "\n",
        "# Define the path to the PDF file\n",
        "pdf_path = '/content/drive/MyDrive/FILE/RWE.pdf'\n",
        "\n",
        "# Function to extract text from a PDF file\n",
        "def extract_text_from_pdf(pdf_path):\n",
        "    text = ''\n",
        "    with pdfplumber.open(pdf_path) as pdf:\n",
        "        for page in pdf.pages:\n",
        "            text += page.extract_text()\n",
        "    return text\n",
        "\n",
        "# Extract text from the PDF file\n",
        "annual_report_text = extract_text_from_pdf(pdf_path)\n",
        "\n",
        "# Define keywords for each tool or methodology\n",
        "tool_keywords_map = {\n",
        "    \"User Interviews\": [\"customer experiences\", \"user feedback\", \"stakeholder engagement\", \"customer satisfaction\", \"user needs\", \"stakeholder perspectives\"],\n",
        "    \"Observational Research\": [\"customer experiences\", \"user feedback\", \"stakeholder engagement\", \"customer satisfaction\", \"user needs\", \"stakeholder perspectives\"],\n",
        "    \"Stakeholder Journey\": [\"customer experiences\", \"user feedback\", \"stakeholder engagement\", \"customer satisfaction\", \"user needs\", \"stakeholder perspectives\"],\n",
        "    \"Empathy Maps\": [\"customer experiences\", \"user feedback\", \"stakeholder engagement\", \"customer satisfaction\", \"user needs\", \"stakeholder perspectives\"],\n",
        "    \"PESTLE Analysis\": [\"market trends\", \"competitive analysis\", \"regulatory changes\", \"economic conditions\", \"industry analysis\", \"external factors\", \"market research\"],\n",
        "    \"SWOT Analysis\": [\"market trends\", \"competitive analysis\", \"regulatory changes\", \"economic conditions\", \"industry analysis\", \"external factors\", \"market research\"],\n",
        "    \"Market Study\": [\"market trends\", \"competitive analysis\", \"regulatory changes\", \"economic conditions\", \"industry analysis\", \"external factors\", \"market research\"],\n",
        "    \"5S Method\": [\"workflow optimization\", \"lean manufacturing\", \"quality control\", \"equipment downtime\", \"process improvement\", \"operational efficiency\", \"waste reduction\"],\n",
        "    \"Heuristic Evaluation\": [\"workflow optimization\", \"lean manufacturing\", \"quality control\", \"equipment downtime\", \"process improvement\", \"operational efficiency\", \"waste reduction\"],\n",
        "    \"Poka-Yoke Error Proofing\": [\"workflow optimization\", \"lean manufacturing\", \"quality control\", \"equipment downtime\", \"process improvement\", \"operational efficiency\", \"waste reduction\"],\n",
        "    \"Single Minute Exchange of Die (SMED)\": [\"workflow optimization\", \"lean manufacturing\", \"quality control\", \"equipment downtime\", \"process improvement\", \"operational efficiency\", \"waste reduction\"],\n",
        "    \"Total Productive Maintenance (TPM)\": [\"workflow optimization\", \"lean manufacturing\", \"quality control\", \"equipment downtime\", \"process improvement\", \"operational efficiency\", \"waste reduction\"],\n",
        "    \"Value Proposition Mapping\": [\"value proposition\", \"strategic objectives\", \"key performance indicators\", \"goal-setting\", \"strategy alignment\", \"business goals\", \"hypothesis-driven\"],\n",
        "    \"Objectives and Key Results (OKRs)\": [\"value proposition\", \"strategic objectives\", \"key performance indicators\", \"goal-setting\", \"strategy alignment\", \"business goals\", \"hypothesis-driven\"],\n",
        "    \"Hypothesis Statement\": [\"value proposition\", \"strategic objectives\", \"key performance indicators\", \"goal-setting\", \"strategy alignment\", \"business goals\", \"hypothesis-driven\"],\n",
        "    \"Event Storming\": [\"creative solutions\", \"brainstorming sessions\", \"innovation workshops\", \"ideation techniques\", \"problem-solving\", \"innovative ideas\", \"creative thinking\"],\n",
        "    \"Detailed Brainstorming\": [\"creative solutions\", \"brainstorming sessions\", \"innovation workshops\", \"ideation techniques\", \"problem-solving\", \"innovative ideas\", \"creative thinking\"],\n",
        "    \"6-3-5 Ideation\": [\"creative solutions\", \"brainstorming sessions\", \"innovation workshops\", \"ideation techniques\", \"problem-solving\", \"innovative ideas\", \"creative thinking\"],\n",
        "    \"Worst Possible Idea\": [\"creative solutions\", \"brainstorming sessions\", \"innovation workshops\", \"ideation techniques\", \"problem-solving\", \"innovative ideas\", \"creative thinking\"],\n",
        "    \"GEMBA Walks\": [\"process optimization\", \"risk assessment\", \"experimental design\", \"continuous improvement\", \"experimentation\", \"risk management\", \"operational analysis\"],\n",
        "    \"What-if Method\": [\"process optimization\", \"risk assessment\", \"experimental design\", \"continuous improvement\", \"experimentation\", \"risk management\", \"operational analysis\"],\n",
        "    \"Design of Experiments\": [\"process optimization\", \"risk assessment\", \"experimental design\", \"continuous improvement\", \"experimentation\", \"risk management\", \"operational analysis\"]\n",
        "}\n",
        "\n",
        "# Function to calculate sentiment score and extract context for a given keyword\n",
        "def analyze_keyword(keyword, text):\n",
        "    matches = re.finditer(keyword, text, re.IGNORECASE)\n",
        "    keyword_context = []\n",
        "    for match in matches:\n",
        "        start_index = max(0, text.rfind(\".\", 0, match.start()) + 1)\n",
        "        end_index = min(len(text), text.find(\".\", match.end()))\n",
        "        context = text[start_index:end_index].strip()\n",
        "        if context:\n",
        "            keyword_context.append(context)\n",
        "    return keyword_context\n",
        "\n",
        "# Function to calculate relevance score for each tool\n",
        "def calculate_relevance_score(tool_keywords_map, text):\n",
        "    # Initialize sentiment analyzer\n",
        "    sid = SentimentIntensityAnalyzer()\n",
        "    # Calculate frequency of keywords for each tool\n",
        "    tool_frequency = {}\n",
        "    for tool, keywords in tool_keywords_map.items():\n",
        "        frequency = sum(text.lower().count(keyword) for keyword in keywords)\n",
        "        tool_frequency[tool] = frequency\n",
        "    # Calculate sentiment score for each tool\n",
        "    tool_sentiment = {}\n",
        "    for tool, keywords in tool_keywords_map.items():\n",
        "        sentiment_scores = []\n",
        "        for keyword in keywords:\n",
        "            context_list = analyze_keyword(keyword, text)\n",
        "            for context in context_list:\n",
        "                sentiment_score = sid.polarity_scores(context)[\"compound\"]\n",
        "                sentiment_scores.append(sentiment_score)\n",
        "        tool_sentiment[tool] = sum(sentiment_scores) / len(sentiment_scores) if sentiment_scores else 0\n",
        "    # Combine frequency and sentiment scores\n",
        "    tool_relevance = {}\n",
        "    for tool in tool_keywords_map.keys():\n",
        "        frequency_score = tool_frequency.get(tool, 0)\n",
        "        sentiment_score = tool_sentiment.get(tool, 0)\n",
        "        relevance_score = frequency_score * 0.7 + sentiment_score * 0.3  # Adjust weights as needed\n",
        "        tool_relevance[tool] = relevance_score\n",
        "    return tool_relevance\n",
        "\n",
        "# Calculate relevance scores for each tool\n",
        "tool_relevance_scores = calculate_relevance_score(tool_keywords_map, annual_report_text)\n",
        "\n",
        "# Rank tools based on relevance score\n",
        "ranked_tools = sorted(tool_relevance_scores.items(), key=lambda x: x[1], reverse=True)\n",
        "\n",
        "# Print ranked tools\n",
        "print(\"Ranked Tools:\")\n",
        "for rank, (tool, score) in enumerate(ranked_tools, 1):\n",
        "    print(f\"{rank}. {tool}: Relevance Score - {score}\")\n"
