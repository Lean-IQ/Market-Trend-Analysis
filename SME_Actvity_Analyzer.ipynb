        "# INNOVATION TYPE ANALYZER #\n",
        "\n",
        "from google.colab import drive\n",
        "import os\n",
        "import pdfplumber\n",
        "import re\n",
        "from collections import defaultdict\n",
        "\n",
        "# Mount Google Drive\n",
        "drive.mount('/content/drive')\n",
        "\n",
        "# Define the path to the PDF file\n",
        "pdf_path = '/content/drive/MyDrive/FILE/RWE.pdf'\n",
        "\n",
        "# Function to extract text from a PDF file\n",
        "def extract_text_from_pdf(pdf_path):\n",
        "    text = ''\n",
        "    with pdfplumber.open(pdf_path) as pdf:\n",
        "        for page in pdf.pages:\n",
        "            text += page.extract_text()\n",
        "    return text\n",
        "\n",
        "# Extract text from the PDF file\n",
        "annual_report_text = extract_text_from_pdf(pdf_path)\n",
        "\n",
        "# Define keywords related to each topic\n",
        "topics_keywords = {\n",
        "    \"Align business and innovation strategies\": [\"ENTER PHRASES HERE\"],\n",
        "    \"Innovation capabilities\": [\"ENTER PHRASES HERE\"],\n",
        "    \"Business capabilities\": [\"ENTER PHRASES HERE"],\n",
        "    \"External networks\": [\"ENTER PHRASES HERE\"],\n",
        "    \"Align cultural and innovation strategies\": [\"ENTER PHRASES HERE\"],\n",
        "    \"Organization and processes\": [\"organizational structure\", \"workflow optimization\", \"cross-functional teams\", \"agile methodology\", \"process automation\", \"lean management\", \"organizational structure\", \"collaboration\", \"processes\"],\n",
        "    \"Integrate functional. business. and geographic silos\": [\"ENTER PHRASES HERE\"]\n",
        "}\n",
        "\n",
        "# Function to perform text analysis and score each topic\n",
        "def analyze_annual_report(report_text, topics_keywords):\n",
        "    topic_scores = defaultdict(int)\n",
        "    for topic, keywords in topics_keywords.items():\n",
        "        for keyword in keywords:\n",
        "            if re.search(keyword, report_text, re.IGNORECASE):\n",
        "                topic_scores[topic] += 1\n",
        "    return topic_scores\n",
        "\n",
        "# Perform text analysis on the annual report\n",
        "report_scores = analyze_annual_report(annual_report_text, topics_keywords)\n",
        "\n",
        "# Print the scores for each topic\n",
        "for topic, score in report_scores.items():\n",
        "    print(f\"{topic}: Score - {score}\")\n",
        "\n",
        "# Calculate Total Scores\n",
        "total_score = sum(report_scores.values())\n",
        "\n",
        "# Normalize Scores\n",
        "normalized_scores = {topic: score / total_score for topic, score in report_scores.items()}\n",
        "\n",
        "# Define reference scores for Need Seeker (NS), Market Reader (MR), and Technology Leader (TL)\n",
        "reference_scores = {\n",
        "    \"NS\": {\n",
        "        \"Align business and innovation strategies\": 4.1,\n",
        "        \"Innovation capabilities\": 3.6,\n",
        "        \"Business capabilities\": 2.3,\n",
        "        \"External networks\": 2.8,\n",
        "        \"Align cultural and innovation strategies\": 2.6,\n",
        "        \"Organization and processes\": 2.1,\n",
        "        \"Integrate functional. business. and geographic silos\": 2.3\n",
        "    },\n",
        "    \"MR\": {\n",
        "        \"Align business and innovation strategies\": 3.5,\n",
        "        \"Innovation capabilities\": 3.6,\n",
        "        \"Business capabilities\": 2.8,\n",
        "        \"External networks\": 2.4,\n",
        "        \"Align cultural and innovation strategies\": 3,\n",
        "        \"Organization and processes\": 2,\n",
        "        \"Integrate functional. business. and geographic silos\": 2.3\n",
        "    },\n",
        "    \"TL\": {\n",
        "        \"Align business and innovation strategies\": 4.5,\n",
        "        \"Innovation capabilities\": 3,\n",
        "        \"Business capabilities\": 3.4,\n",
        "        \"External networks\": 2.9,\n",
        "        \"Align cultural and innovation strategies\": 2.8,\n",
        "        \"Organization and processes\": 2,\n",
        "        \"Integrate functional. business. and geographic silos\": 2.6\n",
        "    }\n",
        "}\n",
        "\n",
        "# Compare normalized scores with reference scores for each innovation type\n",
        "def compare_scores(normalized_scores, reference_scores):\n",
        "    comparison_results = {}\n",
        "    for innovation_type, reference in reference_scores.items():\n",
        "        score_diff = {}\n",
        "        for topic, ref_score in reference.items():\n",
        "            if topic in normalized_scores:\n",
        "                score_diff[topic] = abs(normalized_scores[topic] - ref_score)\n",
        "            else:\n",
        "                score_diff[topic] = 0\n",
        "        total_diff = sum(score_diff.values())\n",
        "        comparison_results[innovation_type] = total_diff\n",
        "    return comparison_results\n",
        "\n",
        "# Compare scores\n",
        "comparison_results = compare_scores(normalized_scores, reference_scores)\n",
        "\n",
        "# Determine the innovation type with the minimum total difference\n",
        "innovation_type = min(comparison_results, key=comparison_results.get)\n",
        "\n",
        "# Output the derived innovation type\n",
        "print(f\"Derived Innovation Type: {innovation_type}\")"
      ]
